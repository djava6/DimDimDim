<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="br.com.useinet.vo.Tipo" table="TIPO">

		<id name="id" column="TIPO_ID">
			<generator class="native" />
		</id>

		<property name="nome"
			not-null="true"
			unique="true" />
			
	</class>

</hibernate-mapping>
